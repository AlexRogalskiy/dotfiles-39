#! /usr/bin/env bash

separator='âœ”\n\n\n'

echo '===== Cleanup brew ====='
brew cleanup
printf $separator

echo '===== Clean npm cache ====='
npm cache clean --force
printf $separator

echo '===== Clean yarn cache ====='
yarn cache clean
printf $separator

echo '===== Clean docker ====='
docker-clean
printf $separator

echo '===== Empty trash ====='
rm -rf ~/.Trash/*
printf $separator
